{
  "name": "trocio",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p routify nollup",
    "build": "routify --single-build --dynamic-imports && rollup -c && routify export",
    "serve": "spassr --serve-spa --serve-ssr",
    "export": "routify export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "db:exemple": "cross-env NODE_ENV=development node ./scripts/dbExemple.js",
    "test": "run-p --race dev cy:run",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
