{
  "name": "trocio",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development run-p serve:api routify nollup",
    "build": "routify --single-build --dynamic-imports && rollup -c && routify export",
    "serve": "run-p serve:api serve:web",
    "serve:api": "nodemon ./api/server.js -w ./api",
    "serve:web": "nodemon server.js",
    "export": "routify export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "db:exemple": "cross-env NODE_ENV=development node ./scripts/dbExemple.js",
    "test": "run-p --race dev cy:run",
    "rollup": "rollup -cw",
    "nollup": "nollup -c",
    "routify": "routify"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@rollup/plugin-replace": "^2.3.3",
    "@sveltech/routify": "^1.9.9",
    "cross-env": "^7.0.2",
    "node-sass": "^4.14.1",
    "nollup": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.28.2",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot": "0.0.31",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-svelte-hot": "^0.9.2",
    "rollup-plugin-terser": "^5.3.0",
    "svelte": "^3.9.1",
    "svelte-material-ui": "^1.0.0-beta.15",
    "svelte-preprocess": "^4.3.2"
  },
  "dependencies": {
    "@rollup/plugin-alias": "^3.1.1",
    "bcrypt": "^5.0.0",
    "bwip-js": "^1.7.3",
    "compression": "^1.7.4",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dayjs": "^1.8.16",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "~4.16.0",
    "express-session": "^1.16.2",
    "got": "^9.6.0",
    "http-errors": "~1.6.2",
    "http-proxy-middleware": "^1.0.5",
    "leaflet": "^1.5.1",
    "mongoose": "^5.10.7",
    "morgan": "~1.9.0",
    "nodemailer": "^6.3.0",
    "pnotify": "^5.1.2",
    "postcss-import": "^12.0.1",
    "print-js": "^1.0.61",
    "qr-scanner": "^1.2.0",
    "qs": "^6.9.4",
    "randomatic": "^3.1.1",
    "spassr": "^1.1.2",
    "swiper": "^6.1.1",
    "uuid": "^8.3.0"
  }
}
